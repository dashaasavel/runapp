{
  "users": [
    {
      "name": "user",
      "password": "password",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts":[
    {"name":"/"}
  ],
  "queues": [
    {
      "name": "user-deleted-queue-mail-service",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "user-deleted-queue-run-service",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "user-unconfirmed-queue-mail-service",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "user-created-queue-mail-service",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    }
  ],
  "permissions": [
    {
      "user": "user",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "user.deleted",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "user.created",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "user.unconfirmed",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "user.deleted",
      "vhost": "/",
      "destination": "user-deleted-queue-run-service",
      "destination_type": "queue",
      "routing_key": "user.event.deleted",
      "arguments": {}
    },
    {
      "source": "user.deleted",
      "vhost": "/",
      "destination": "user-deleted-queue-mail-service",
      "destination_type": "queue",
      "routing_key": "user.event.deleted",
      "arguments": {}
    },
    {
      "source": "user.unconfirmed",
      "vhost": "/",
      "destination": "user-unconfirmed-queue-mail-service",
      "destination_type": "queue",
      "routing_key": "user.event.unconfirmed",
      "arguments": {}
    },
    {
      "source": "user.created",
      "vhost": "/",
      "destination": "user-created-queue-mail-service",
      "destination_type": "queue",
      "routing_key": "user.event.created",
      "arguments": {}
    }
  ]
}